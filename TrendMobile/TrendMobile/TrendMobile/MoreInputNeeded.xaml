<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TrendMobile.MoreInputNeeded">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="AdditionalEntry"></Entry>
            <Button x:Name="OkayButton" Text="Okay"></Button>
            <Button x:Name="CancelButton" Text="Cancel"></Button>
            <Button x:Name="DeleteButton" Text="Delete"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>